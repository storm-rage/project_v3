.land-container
  width 100%
  height 100%
  .search-box
    height 55px
    white-space nowrap
    overflow hidden
    span
      margin-right 6px
    .el-input,.el-select
      display inline-block
      width 220px
      margin-right 20px
  .control-box
    height 45px
    white-space nowrap
    overflow hidden
  .table-box
    height calc(100% - 100px)
    .tips
      font-size 14px
      height 30px
      color #333333
      white-space nowrap
      overflow hidden
      .number
          color #000000
          font-weight bold
          padding 0px 6px
      .empty
          cursor pointer
          color #02A1E9
          margin-left 6px
    .table
      height calc(100% - 85px)
    .pagination
      text-align right
      height 35px
      padding-top 20px
      white-space nowrap
      overflow hidden
  .edit-dialog
    height 90%
    .el-dialog__body
      height calc(100% - 115px)